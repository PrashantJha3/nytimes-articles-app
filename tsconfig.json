    "include": ["src/**/*.ts", "src/**/*.tsx", "src/components/ArticleDetail.js", "src/components/ArticlesList.js", "src/services/nytimesService.js", "src/components/ArticleItem.js", "src/store/index.js", "src/store/articlesSlice.js", "src/index.js"]
  }